<?xml version="1.0" encoding="GBK"?>
<stream>
	<status></status><!--交易状态 char(7) -->
	<statusText></statusText><!--交易状态信息 varchar(254) -->
	<list name="userDataList">
		<row>
			<mctNo></mctNo><!--商户号 char(8) -->
			<mctJnlNo></mctJnlNo><!--商户流水号 varchar(20) -->
			<tranDate></tranDate><!--交易日期 char(8) -->
			<tranTime></tranTime><!--交易时间char(8) -->
			<payAccNo></payAccNo><!--付款账户 char(19) -->
			<payAccNm></payAccNm><!--付款账户名称 varchar(100) -->
			<recvAccNo></recvAccNo><!--收款账户 char(19) -->
			<recvAccNm></recvAccNm><!--收款账户名称 varchar(100) -->
			<tranAmt></tranAmt><!--交易金额 decimal(15,2) -->
			<abstract></abstract><!--摘要 char(22) -->
			<retFlag></retFlag><!--退还标志 char(1)，0：未退还；1：已退还 -->
			<cryType></cryType><!--币种 char(2) -->
			<xtrvfg></xtrvfg><!--被冲账标识 char(1) ，Y：已冲账；N：未冲账 -->
		</row>
	</list>
</stream>