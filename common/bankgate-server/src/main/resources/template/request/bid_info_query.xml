<?xml version="1.0" encoding="GBK"?>
<stream>
	<action>DLDPDQRY</action>
	<userName></userName><!--登录名 varchar(30) -->
	<qryType></qryType><!--查询类型char(1)，1：精确查询；2：模糊查询 -->
	<mctJnlNo></mctJnlNo><!--商户流水号 varchar(20)，可空，当qryType为1时必输 -->
